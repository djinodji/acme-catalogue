<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AlbumPhoto" table="Album_photo">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="albumId" type="integer" column="Album_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="imageId" type="integer" column="Image_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="order" type="integer" column="order" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="statut" type="integer" column="statut" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
