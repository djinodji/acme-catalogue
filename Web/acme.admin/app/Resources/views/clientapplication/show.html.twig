{% extends 'base.html.twig' %}

{% block body %}
    <h1>ClientApplication</h1>

    <table>
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ clientApplication.nom }}</td>
            </tr>
            <tr>
                <th>Uidapplication</th>
                <td>{{ clientApplication.uidApplication }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ clientApplication.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('clientapplication_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('clientapplication_edit', { 'id': clientApplication.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
